{
    "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "environment": {
            "value": "dev"
        },
        //"adminPublicKey": {
        //    "value": ""
        //},
        "networkSecurityGroupRules": {
            "value": [
                {
                    "name": "Allow-HTTPS",
                    "properties": {
                        "description": "Allow HTTPS to the internet",
                        "priority": 200,
                        "protocol": "TCP",
                        "access": "Allow",
                        "direction": "Outbound",
                        "sourceAddressPrefix": "*",
                        "sourcePortRange": "443",
                        "destinationAddressPrefix": "Internet",
                        "destinationPortRange": "443"
                    }
                }
            ]
        },
        "customScriptFileUri": {
            "value": "https://raw.githubusercontent.com/ryanmangan/github-actions-practice/main/bash_scripts/autopart.sh"
        },
        "commandToExecute": {
            "value": "bash autopart.sh"
        }
    }
}
